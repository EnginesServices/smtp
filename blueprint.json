{
  "title": "mysql server",
  "description": "mysql",
  "memory_usage": {
    "required": 64,
    "recommended": 96
  },
  "licenses": [
  ],
  "bindings": [
    {
      "identifier": "spool",
      "descriptor": {
        "repository": "https://github.com/EnginesServices/filesystem.git",
        "branch": "spaces"
      },
      "variables": {
        "path": "/var/^^identifier^^/postfix/",
        "user": "postfix",
        "group": "postfix",
        "fw_user": "postfix"
      }
    },
    {
      "identifier": "dkim",
      "descriptor": {
        "repository": "https://github.com/EnginesServices/filesystem.git",
        "branch": "spaces"
      },
      "variables": {
        "path": "/etc/opendkim/keys",
        "user": "",
        "group": "postfix",
        "fw_user": "postfix"
      }
    }
  ],
  "stages": [
    {
      "starter": {
        "image": "engines/servicebase",
        "tag": "current"
      },
      "state": {
        "type_path": "smtp",
        "deployment_type": "worker",
        "default_stop_timeout": 0,
        "restart_attempts": 0
      },
      "disposition": [
        "attach_post_build",
        "start_syslog"
      ],
      "os_packages": [
        {
          "name": "postfix"
        },
        {
          "name": "opendkim"
        },
        {
          "name": "opendkim-tools"
        }
      ],
      "log_directories": [
        "/var/log"
      ],
      "custom_files": [
        {
          "type": "signal",
          "path": "somehwyere whay is  this dor",
          "language": "sh",
          "content": "custome ?",
          "execute": true,
          "sudo": true
        }
      ]
    ]
  }
}
